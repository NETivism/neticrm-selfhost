# MYSQL Settings
MYSQL_ROOT_PASSWORD=changeThis8964
MYSQL_DATABASE=neticrmdb
MYSQL_USER=neticrmdb
MYSQL_PASSWORD=password1234

# ADMIN CREDENTIALS
# Used for Drupal admin user
ADMIN_LOGIN_USER=neticrm_admin
ADMIN_LOGIN_PASSWORD=SecurePassword123!

# PORT
# Set the IP address to bind to. Leave blank to bind to all interfaces (0.0.0.0).
# Note: Use separate fields for IP and PORT.
# Do not use IP:PORT format in HTTP_BIND or HTTP_PORT.
HTTP_PORT=8080
# HTTP_BIND=

# DOMAIN
# Used by drush with the -l flag
DOMAIN=domain.name

# LANGUAGE
# Language code for Drupal installation, can be set to zh-hant or other language codes
# Default is en if not specified
LANGUAGE=en